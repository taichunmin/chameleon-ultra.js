{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": false,
  "customCss": "./build-utils/typedoc-custom.css",
  "darkHighlightTheme": "dark-plus",
  "gaID": "G-KB8ZL6LPH5",
  "includeVersion": true,
  "out": "dist",
  "entryPoints": [
    "src/index.ts",
    "src/Crypto1.ts",
    "src/plugin/Debug.ts",
    "src/plugin/DfuZip.ts",
    "src/plugin/SerialPortAdapter.ts",
    "src/plugin/WebbleAdapter.ts",
    "src/plugin/WebserialAdapter.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/node_modules/**"
  ],
  "groupOrder": [
    "Constructors",
    "Properties",
    "Connection Related",
    "Device Related",
    "Slot Related",
    "Reader Related",
    "Emulator Related",
    "Mifare Classic Related",
    "Mifare Ultralight Related",
    "DFU Related",
    "Plugin Related",
    "Methods",
    "*"
  ],
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Class",
    "Enum",
    "EnumMember",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "navigationLinks": {
    "Demos": "https://taichunmin.idv.tw/chameleon-ultra.js/documents/Demos.html",
    "GitHub": "https://github.com/taichunmin/chameleon-ultra.js",
    "NPM": "https://www.npmjs.com/package/chameleon-ultra.js",
    "jsDelivr": "https://www.jsdelivr.com/package/npm/chameleon-ultra.js"
  },
  "plugin": [
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults",
    "typedoc-plugin-ga"
  ],
  "projectDocuments": [
    "documents/*.md"
  ],
  "sort": [
    "kind",
    "visibility",
    "instance-first",
    "enum-value-ascending",
    "alphabetical-ignoring-documents"
  ]
}
